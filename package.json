{
  "name": "feeble",
  "version": "0.3.0",
  "description": "React + Redux Architecture",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src test",
    "test": "jest",
    "test:examples": "babel-node examples/testAll.js",
    "check:src": "yarn lint && yarn test",
    "check:examples": "yarn build:examples && yarn test:examples",
    "prebuild": "yarn clean",
    "build": "babel src --out-dir lib",
    "build:examples": "babel-node examples/buildAll.js",
    "prepublish": "yarn check:src && yarn check:examples && yarn build",
    "docs:prepare": "gitbook install",
    "docs:watch": "yarn docs:prepare && gitbook serve",
    "docs:build": "yarn docs:prepare && rimraf _book && gitbook build",
    "docs:publish": "yarn docs:build && cd _book && git init && git commit --allow-empty -m 'Update docs' && git checkout -b gh-pages && git add . && git commit -am 'Update docs' && git push git@github.com:tianche/feeble gh-pages --force"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "testPathDirs": [
      "<rootDir>/test"
    ]
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Wei Zhu <yesmeck@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.1",
    "jest": "^18.1.0",
    "lodash": "^4.13.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.0",
    "reselect": "^2.5.3"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "enzyme": "^2.4.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "gitbook-cli": "^2.3.0",
    "jsdom": "^9.4.1",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.0",
    "rimraf": "^2.5.4"
  },
  "peerDependencirs": {
    "react": "^0.14.0 || ^15.0.0-0"
  }
}
